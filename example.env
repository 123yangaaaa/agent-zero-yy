# OpenAI and other major AI providers
API_KEY_OPENAI=
API_KEY_ANTHROPIC=
API_KEY_GROQ=
API_KEY_PERPLEXITY=
API_KEY_GOOGLE=
API_KEY_MISTRAL=
API_KEY_OPENROUTER=
API_KEY_SAMBANOVA=

# Deepseek and Zhipu API keys (required)
API_KEY_DEEPSEEK=your-deepseek-api-key  # 替换为您的 Deepseek API 密钥
API_KEY_ZHIPU=your-zhipu-api-key  # 替换为您的智谱 API 密钥

# Azure OpenAI configuration
API_KEY_OPENAI_AZURE=
OPENAI_AZURE_ENDPOINT=
OPENAI_API_VERSION=

# Hugging Face token
HF_TOKEN=

# Web UI configuration
WEB_UI_PORT=50001

# System configuration
TOKENIZERS_PARALLELISM=true
PYDEVD_DISABLE_FILE_VALIDATION=1

# API Base URLs
OLLAMA_BASE_URL="http://127.0.0.1:11434"
LM_STUDIO_BASE_URL="http://127.0.0.1:1234/v1"
OPEN_ROUTER_BASE_URL="https://openrouter.ai/api/v1"
SAMBANOVA_BASE_URL="https://fast-api.snova.ai/v1"

# Model selection (required)
MODEL_TYPE=deepseek-chat  # 可选值: deepseek-chat, deepseek-coder, glm4v
IMAGE_PATH=  # 仅在使用 glm4v 时需要
IMAGE_PROMPT=  # 仅在使用 glm4v 时需要
